<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="robots" content="noindex,nofollow">
    <title>Автобагаж - купить автобагажники и автобоксы на крышу автомобиля в Перми - @ViewData["Title"]</title>

    <link rel="stylesheet" href="~/fa/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/build/autobagaz.css" />

    <script type="text/javascript" src="//vk.com/js/api/openapi.js?139"></script>
    <script type="text/javascript">
    VK.init({apiId: 5866168, onlyWidgets: true});
    </script>
    <script src="~/jquery/jquery.min.js" type="text/javascript"></script>
</head>
<body>
    <header class="header">
        @{await Html.RenderPartialAsync("_Header");}
        @{await Html.RenderPartialAsync("_Proposition");}
    </header>
    <div class="wrapper">
        @{await Html.RenderPartialAsync("_LeftNavigation");}
        <main role="main" class="wrapper__content">
            @RenderBody()
        </main>
    </div>
    @{await Html.RenderPartialAsync("_Footer");}
    <script src="~/scripts/placeholder.js"></script>
</body>
</html>
